{
  "name": "electron-ps-rs",
  "version": "0.1.0",
  "description": "Example electron app with PureScript and Rust",
  "main": "index.node",
  "scripts": {
    "test": "cargo test",
    "cargo-build": "cargo build --message-format=json > cargo.log",
    "cross-build": "cross build --message-format=json > cross.log",
    "postcargo-build": "neon dist < cargo.log",
    "postcross-build": "neon dist -m /target < cross.log",
    "debug": "npm run cargo-build --",
    "build": "npm run cargo-build -- --release",
    "cross": "npm run cross-build -- --release"
  },
  "author": "nathaniel may",
  "license": "MIT",
  "devDependencies": {
    "@neon-rs/cli": "0.1.73"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathaniel-may/electron-ps-rs.git"
  },
  "keywords": [
    "electron",
    "purescript",
    "rust"
  ],
  "bugs": {
    "url": "https://github.com/nathaniel-may/electron-ps-rs/issues"
  },
  "homepage": "https://github.com/nathaniel-may/electron-ps-rs#readme",
  "dependencies": {
    "electron": "^31.2.0"
  }
}
